{
  "_version": "1.65.0",
  "sap.app": {
    "id": "materialcreation",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.4",
      "toolsId": "8b58341b-d651-41ea-b109-77f60020a466"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/odata/v4/MaterialSrv/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "materialcreationsem-display": {
          "semanticObject": "materialcreationsem",
          "action": "display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.130.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "materialcreation.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "materialcreation.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteloginPage",
          "pattern": ":?query:",
          "target": [
            "TargetloginPage"
          ]
        },
        {
          "name": "MaterialCreationView",
          "pattern": "MaterialCreationView",
          "target": [
            "MaterialCreationView"
          ]
        },
        {
          "name": "MaterialDetails",
          "pattern": "MaterialDetails",
          "target": [
            "MaterialDetails"
          ]
        }
      ],
      "targets": {
        "TargetloginPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "loginPage",
          "viewName": "loginPage"
        },
        "MaterialCreationView": {
          "viewType": "XML",
          "viewName": "MaterialCreationView",
          "viewPath": "materialcreation.view"
        },
        "MaterialDetails": {
          "viewType": "XML",
          "viewName": "MaterialDetails",
          "viewPath": "MaterialDetails.view"
        }
      }
    },
    "rootView": {
      "viewName": "materialcreation.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}